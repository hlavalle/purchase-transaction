{
  "mdc": {
    "$resolver": "mdc"
  },
  "exceptionClass": {
    "$resolver": "exception",
    "field": "className"
  },
  "stackTrace": {
    "$resolver": "exception",
    "field": "stackTrace"
  },
  "message": {
    "$resolver": "message",
    "stringified": true
  },
  "thread": {
    "$resolver": "thread",
    "field": "name"
  },
  "timestamp": {
    "$resolver": "timestamp",
    "pattern": {
      "format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'",
      "timezone": "UTC"
    },
    "level": {
      "$resolver": "level",
      "field": "name"
    },
    "logger": {
      "$resolver": "logger",
      "field": "name"
    }
  }
}